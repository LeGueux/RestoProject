<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>Resto</display-name>
	<welcome-file-list>
		<welcome-file>connexion.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>AddPlatServlet</servlet-name>
		<servlet-class>Servlet.AddPlatServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UtilisateurServlet</servlet-name>
		<servlet-class>Servlet.UtilisateurServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ConnexionServlet</servlet-name>
		<servlet-class>Servlet.ConnexionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddPlatServlet</servlet-name>
		<url-pattern>/AddPlatServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddPlatServlet</servlet-name>
		<url-pattern>/PlatServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UtilisateurServlet</servlet-name>
		<url-pattern>/UtilisateurServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UtilisateurServlet</servlet-name>
		<url-pattern>/Account</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ConnexionServlet</servlet-name>
		<url-pattern>/Deconnexion</url-pattern>
	</servlet-mapping>
</web-app>